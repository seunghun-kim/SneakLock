{
  "schemaVersion": 1,
  "id": "sneaklock",
  "version": "${version}",
  "name": "SneakLock",
  "description": "Toggle sneak with both shift keys and prevent jumping while sneaking",
  "authors": [
    "Seunghun Kim"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/sneaklock/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "org.teamck.sneaklock.client.SneaklockDataGenerator"
    ],
    "client": [
      "org.teamck.sneaklock.client.SneaklockClient"
    ],
    "main": [
      "org.teamck.sneaklock.Sneaklock"
    ]
  },
  "mixins": [
    "sneaklock.mixins.json",
    {
      "config": "sneaklock.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
